<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <tab_fragment
        android:id="@+id/navigation_home"
        android:name="me.tang.wanandroid.ui.main.home.HomeFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home" />

    <tab_fragment
        android:id="@+id/navigation_system"
        android:name="me.tang.wanandroid.ui.main.system.SystemFragment"
        android:label="@string/system"
        tools:layout="@layout/fragment_system" />

    <tab_fragment
        android:id="@+id/navigation_discovery"
        android:name="me.tang.wanandroid.ui.main.discovery.DiscoveryFragment"
        android:label="@string/discovery"
        tools:layout="@layout/fragment_discovery" />

    <tab_fragment
        android:id="@+id/navigation_navigation"
        android:name="me.tang.wanandroid.ui.main.navigation.NavigationFragment"
        android:label="@string/navigation"
        tools:layout="@layout/fragment_navigation" />

    <tab_fragment
        android:id="@+id/navigation_mine"
        android:name="me.tang.wanandroid.ui.main.mine.MineFragment"
        android:label="@string/mine"
        tools:layout="@layout/fragment_mine" />

</navigation>